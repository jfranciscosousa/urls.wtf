{
  "name": "urls.wtf",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "clean": "rm -rf .svelte-kit .vercel node_modules",
    "dev": "vite dev",
    "build": "vite build",
    "build-prod": "prisma generate --no-engine && prisma db push && vite build",
    "preview": "vite preview",
    "ts-check": "prisma generate && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "ts-check:watch": "prisma generate && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "ci": "npm run lint && npm run ts-check && npm run build"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.7.2",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prisma": "^6.8.2",
    "svelte": "^5.33.2",
    "svelte-check": "^4.2.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "6.8.2",
    "@prisma/extension-accelerate": "^2.0.0",
    "@tailwindcss/postcss": "^4.1.7"
  }
}

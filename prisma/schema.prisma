generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma-generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model HashedUrl {
  id        String     @id @default(uuid())
  hash      String     @unique
  url       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
